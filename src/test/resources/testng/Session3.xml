<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Suite1" verbose="1">

    <!--LISTENERS-->
    <listeners>
        <listener class-name="org.automation.configurations.testng.CustomListener" />
    </listeners>

    <!-- GLOBAL PARAMETERS -->
    <parameter name="browser" value="chrome"/>
    <parameter name="app_admin_url" value="http://127.0.0.1:9000/admin/"/>
    <parameter name="username" value="automation"/>
    <parameter name="password" value="Automation123$"/>

    <!-- TESTS -->
    <test name="full_regression">

        <!-- WHAT TO RUN -->
        <groups>
            <run>
                <include name="dp_excel"/>
            </run>
        </groups>

        <classes>
            <class name="org.automation.testcases.LoginSuite"></class>
            <class name="org.automation.testcases.Session3Suite01"></class>
        </classes>
    </test>
</suite>